{
    "manifestVersion": 1,
    "id": "BoostSecurityScan",
    "name": "BoostSecurity Scanner",
    "version": "1.0.1",
    "public": true,
    "publisher": "BoostSecurity",
    "description": "BoostSecurity scanner for azure pipelines",
    "categories": [
        "Azure Pipelines"
    ],
    "content": {
      "details": {
        "path": "listing/overview.md"
      }
    },
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "BoostSecurityScan"
            }
        }
    ],
    "files": [
        {
            "path": "BoostSecurityScan"
        }
    ],
    "icons": {
      "default": "listing/boostsecurity-logo.png"
    },
    "links": {
      "home": {
        "uri": "https://www.boostsecurity.io"
      },
      "documentation": {
        "uri": "https://docs.boostsecurity.io"
      }
    },
    "repository": {
      "type": "git",
      "uri": "https://github.com/boostsecurityio/boostsec-scanner-azure"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ]
}
